{
  "name": "llm-cmp",
  "version": "1.0.0",
  "description": "LLM Orchestration via Cortical Messaging Protocol",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/index.js",
    "start": "node dist/index.js",
    "demo:mcp-integration": "tsc && node dist/demo/mcp-integration-demo.js",
    "debug:mcp-integration": "tsc && node --inspect-brk=9229 --enable-source-maps dist/demo/mcp-integration-demo.js",
    "test:agents": "tsc && node dist/tests/agents/run-agent-tests.js",
    "test:agents:quick": "tsc && node dist/tests/agents/run-agent-tests.js quick",
    "test:agents:week2": "tsc && node dist/tests/agents/run-agent-tests.js week2",
    "test:htm": "tsc && node dist/tests/htm/run-htm-tests.js",
    "test:htm:quick": "tsc && node dist/tests/htm/run-htm-tests.js quick",
    "test:htm:week1": "tsc && node dist/tests/htm/run-htm-tests.js week1"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.5.3"
  },
  "private": true
}
